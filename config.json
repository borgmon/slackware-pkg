{
  "output_path": "./build",
  "temp_path": "./tmp",
  "packages": [
    {
      "name": "ripgrep",
      "git_url": "https://github.com/BurntSushi/ripgrep.git",
      "tag": "14.1.1",
      "description": "ripgrep recursively searches directories for a regex pattern while respecting your gitignore.",
      "build": 1,
      "enabled": true,
      "build_env": "rust",
      "build_command": "cargo build --release --features pcre2",
      "bin_path": "target/release/rg"
    },
    {
      "name": "yazi",
      "git_url": "https://github.com/sxyazi/yazi.git",
      "tag": "v25.5.31",
      "description": "Blazing fast terminal file manager written in Rust, based on async I/O.",
      "build": 1,
      "enabled": true,
      "build_env": "rust",
      "build_command": "cargo build --release",
      "bin_path": "target/release/yazi"
    },
    {
      "name": "lsd",
      "git_url": "https://github.com/lsd-rs/lsd.git",
      "tag": "v1.2.0",
      "description": "The next gen ls command.",
      "build": 1,
      "enabled": true,
      "build_env": "rust",
      "build_command": "cargo build --release",
      "bin_path": "target/release/lsd"
    },
    {
      "name": "fd",
      "git_url": "https://github.com/sharkdp/fd.git",
      "tag": "v10.3.0",
      "description": "A simple, fast and user-friendly alternative to 'find'.",
      "build": 1,
      "enabled": true,
      "build_env": "rust",
      "build_command": "cargo build --release",
      "bin_path": "target/release/fd"
    },
    {
      "name": "zoxide",
      "git_url": "https://github.com/ajeetdsouza/zoxide.git",
      "tag": "v0.9.8",
      "description": "A smarter cd command. Supports all major shells.",
      "build": 1,
      "enabled": true,
      "build_env": "rust",
      "build_command": "cargo build --release",
      "bin_path": "target/release/zoxide"
    },
    {
      "name": "resvg",
      "git_url": "https://github.com/linebender/resvg.git",
      "tag": "v0.45.1",
      "description": "An SVG rendering library.",
      "build": 1,
      "enabled": true,
      "build_env": "rust",
      "build_command": "cargo build --release",
      "bin_path": "target/release/resvg"
    },
    {
      "name": "micro",
      "git_url": "https://github.com/zyedidia/micro.git",
      "tag": "v2.0.14",
      "description": "A modern and intuitive terminal-based text editor.",
      "build": 1,
      "enabled": true,
      "build_env": "go",
      "build_command": "make build",
      "bin_path": "micro"
    },
    {
      "name": "dust",
      "git_url": "https://github.com/bootandy/dust.git",
      "tag": "v1.2.3",
      "description": "A more intuitive version of du in rust.",
      "build": 1,
      "enabled": true,
      "build_env": "rust",
      "build_command": "cargo build --release",
      "bin_path": "target/release/dust"
    },
    {
      "name": "starship",
      "git_url": "https://github.com/starship/starship.git",
      "tag": "v1.23.0",
      "description": "The minimal, blazing-fast, and infinitely customizable prompt for any shell!",
      "build": 1,
      "enabled": true,
      "build_env": "rust",
      "build_command": "cargo build --release",
      "bin_path": "target/release/starship"
    },
    {
      "name": "lazydocker",
      "git_url": "https://github.com/jesseduffield/lazydocker.git",
      "tag": "v0.24.1",
      "description": "The lazier way to manage everything docker.",
      "build": 1,
      "enabled": true,
      "build_env": "go",
      "build_command": "CGO_ENABLED=0 GOOS=linux go build",
      "bin_path": "lazydocker"
    },
    {
      "name": "lazydocker",
      "git_url": "https://github.com/jesseduffield/lazydocker.git",
      "tag": "v0.24.0",
      "description": "The lazier way to manage everything docker.",
      "build": 1,
      "enabled": true,
      "build_env": "go",
      "build_command": "CGO_ENABLED=0 GOOS=linux go build",
      "bin_path": "lazydocker"
    },
    {
      "name": "lazygit",
      "git_url": "https://github.com/jesseduffield/lazygit.git",
      "tag": "v0.55.1",
      "description": "simple terminal UI for git commands.",
      "build": 1,
      "enabled": true,
      "build_env": "go",
      "build_command": "CGO_ENABLED=0 GOOS=linux go build",
      "bin_path": "lazygit"
    }
  ]
}
